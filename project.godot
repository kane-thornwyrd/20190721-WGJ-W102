; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "ActionList",
"language": "GDScript",
"path": "res://scripts/action_list.gd"
}, {
"base": "Sprite",
"class": "Echo",
"language": "GDScript",
"path": "res://assets/fx/echo.gd"
}, {
"base": "Node",
"class": "FireE",
"language": "GDScript",
"path": "res://scripts/player/states/fire_e.gd"
}, {
"base": "HBoxContainer",
"class": "InputLine",
"language": "GDScript",
"path": "res://scripts/input_line.gd"
}, {
"base": "Panel",
"class": "KeySelector",
"language": "GDScript",
"path": "res://scripts/key_selector.gd"
}, {
"base": "Node",
"class": "KeymapInstaller",
"language": "GDScript",
"path": "res://scripts/globals/keymap_installer.gd"
}, {
"base": "Node",
"class": "Level",
"language": "GDScript",
"path": "res://scripts/level.gd"
}, {
"base": "Button",
"class": "NavigationButton",
"language": "GDScript",
"path": "res://scripts/nav_button.gd"
}, {
"base": "KinematicBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://scripts/player/player.gd"
}, {
"base": "Node",
"class": "PlayerFSM",
"language": "GDScript",
"path": "res://scripts/player/player_fsm.gd"
}, {
"base": "Button",
"class": "QuitButton",
"language": "GDScript",
"path": "res://scripts/quit_button.gd"
}, {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://addons/kane-FSM/state.gd"
}, {
"base": "Node",
"class": "StateMachine",
"language": "GDScript",
"path": "res://addons/kane-FSM/state_machine.gd"
} ]
_global_script_class_icons={
"ActionList": "",
"Echo": "",
"FireE": "",
"InputLine": "",
"KeySelector": "",
"KeymapInstaller": "",
"Level": "",
"NavigationButton": "",
"Player": "",
"PlayerFSM": "",
"QuitButton": "",
"State": "",
"StateMachine": "res://addons/kane-FSM/fsm_icon.svg"
}

[application]

config/name="Omega Turbo Force"
run/main_scene="res://main.tscn"
boot_splash/bg_color=Color( 0.988235, 0, 0, 1 )
config/icon="res://icon.png"

[autoload]

level_reg="*res://scripts/globals/level_registery.gd"
scene_reg="*res://scripts/globals/scene_registery.gd"
UTILS="*res://scripts/globals/utils.gd"
keymap_installer="*res://scripts/globals/keymap_installer.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/borderless=true
window/size/fullscreen=true
window/size/test_width=1365
window/size/test_height=768
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true
window/per_pixel_transparency/splash=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "kane-FSM" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[node]

name_casing=2

[rendering]

quality/2d/gles2_use_nvidia_rect_flicker_workaround=true
quality/2d/use_pixel_snap=true
quality/filters/use_nearest_mipmap_filter=true
environment/default_environment="res://assets/environments/bloom.tres"
